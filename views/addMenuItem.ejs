<html>
	<head>
		<meta charset="utf-8">
	    <meta http-equiv="X-UA-Compatible" content="IE=edge">
	    <meta name="viewport" content="width=device-width, initial-scale=1">
	    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
	    <meta name="description" content="">
	    <meta name="author" content="Zhen Wei">
	    <link rel="icon" href="/images/icons/favicon.jpg">
	    <title>Add Menu Item</title>
	</head>

	<body ng-app="angularApp">
		<div ng-controller="AddMenuController">
			<form ng-submit="signIn()">
				<fieldset>
					<legend>Personal information:</legend>
					name:<br>
					<input type="text" name="name" ng-model="item.name">
					<br>
					type:<br>
					<input type="text" name="type" ng-model="item.type">
					<br>
					restaurant:<br>
					<input type="text" name="restaurant" ng-model="item.restaurant">
					<br>
					cal:<br>
					<input type="text" name="cal" ng-model="item.cal">
					<br>
					price:<br>
					<input type="text" name="price" ng-model="item.price">
					<br>
					zone:<br>
					<input type="text" name="zone" ng-model="item.zone">
					<br>
					imgURL:<br>
					<input type="text" name="imgURL" ng-model="item.imgURL">
					<br><br>
					<input type="submit" value="Submit">
				</fieldset>
			</form>
		</div>

