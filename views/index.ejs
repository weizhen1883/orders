        <form action="#" method="POST" class="navbar-form navbar-right">
          <div class="form-group login-action-hidden">
            <input type="text" name="login_email" placeholder="Email" class="form-control">
          </div>
          <div class="form-group login-action-hidden">
            <input type="password" name="login_password" placeholder="Password" class="form-control">
          </div>
          <button type="submit" class="btn btn-default login-action-hidden">Sign in</button>
          <button type="button" class="btn btn-default login-action hidden" id="user-button"></button>
          <button type="button" class="btn btn-default login-action hidden">Sign Out</button>
        </form>
      </div>
